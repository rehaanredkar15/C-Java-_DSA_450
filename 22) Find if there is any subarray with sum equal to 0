 bool subArrayExists(int a[], int n)
    {
        //Your code here
         int s  =0;
         unordered_map<int,int> m;
         for(int i = 0;i<n;i++)
         {
             s = s + a[i];
             if(a[i]==0 || s==0 || m[s])
             {
                 
                 return true;
                 break;
            
             }
             else
             {
                 m[s] = 1;
                 
             }
         }
         return false;
        
    }
