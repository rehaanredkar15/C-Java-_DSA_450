class Result {
    int firstUniqueChar(char *str){
    
    int n = strlen(str);
    int count[26] = {0};
    
    for(int i = 0;i<n;i++)
    {
      count[str[i] - 'a']++;  //this gives the ascii value of  the character of the letter and we can increment its value 
    }  
    
    for(int i = 0;i<n;i++)
    {
      if(count[str[i] - 'a'] == 1)
        return i;
    }
    
    return -1;
  }
}
